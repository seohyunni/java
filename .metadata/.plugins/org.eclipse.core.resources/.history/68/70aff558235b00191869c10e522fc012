package lsh;

import java.io.IOException;
import java.util.Scanner;

public class IOProgram {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner scan = new Scanner(System.in);
		
		int bt ; 
		try {
			bt = System.in.read();
			System.out.println((char)bt);
			
		} catch (IOException e) {
			e.printStackTrace();
		}
	}

}
